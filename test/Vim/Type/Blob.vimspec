Describe Vim.Type.Blob
  Before all
    let b = vital#vital#new().import('Vim.Type.Blob')
  End

  Describe .new()
    It cast any data
      for i in [1,2,4,8,16,32,64]
        Assert Equals(len(b.new(i)), i)
      endfor
    End
  End

